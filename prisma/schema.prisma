generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        BigInt   @id
  name      String
  email     String   @unique
  username  String   @unique
  imageUrl  String   @map("image_url")

  createdAt DateTime @default(now()) @map("created_at")
}
